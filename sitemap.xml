---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ site.url.host }}{{ site.url.prefix }}</loc>
    </url>
    
    {% for page in site.pages %}
    <url>
        <loc>{{ site.url.host }}{{ site.url.prefix }}{{ page.url }}</loc>
    </url>
    {% endfor %}

    {% for post in site.posts %}
    <url>
        <loc>{{ site.url.host }}{{ site.url.prefix }}{{ post.url }}</loc>
    </url>
    {% endfor %}
</urlset>
